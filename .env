# ============================================================
# .env — non-secret defaults
# Committed to git. Do NOT put passwords or keys here.
# Secrets and local overrides belong in .env.local (git-ignored).
# ============================================================

# ── Versions ─────────────────────────────────────────────────
MINIO_VERSION=latest
POSTGRES_VERSION=16-alpine
ICEBERG_REST_VERSION=0.10.0
TRINO_VERSION=435

# ── Ports ────────────────────────────────────────────────────
MINIO_API_PORT=9000
MINIO_CONSOLE_PORT=9001
POSTGRES_PORT=5432
ICEBERG_REST_PORT=8181
TRINO_PORT=8080
AIRFLOW_API_SERVER_PORT=8080
FLUENTD_PORT=24224

# ── MinIO ────────────────────────────────────────────────────
MINIO_DEFAULT_BUCKET=iceberg-warehouse

# ── AWS / region (used by Iceberg REST + Trino) ──────────────
AWS_REGION=us-east-1

# ── Airflow ──────────────────────────────────────────────────
AIRFLOW_VAR_ENVIRONMENT=local